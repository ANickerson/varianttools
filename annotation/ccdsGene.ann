[linked fields]
hg19=chr, cds_from, cds_to

[data sources]
anno_type=range
description=CCDS gene
version=20110422
source_url=ftp://ftp.ncbi.nlm.nih.gov/pub/CCDS/archive/HsGRCH37.2/CCDS.20110422.txt
direct_url=http://vtools.houstonbioinformatics.org/annoDB/ccdsGene-20110420.DB.gz
source_type=txt

[chr]
index=1
type=chromosome

[nc_accession]
index=2
type=VARCHAR(255)
comment=nc accession

[gene]
index=3
type=VARCHAR(255)
comment=gene name

[gene_id]
index=4
type=INTEGER
comment=gene id

[ccds_id]
index=5
type=VARCHAR(255)
comment=ccds id

[ccds_status]
index=6
type=VARCHAR(20)
comment=ccds status, can be Public, Reviewed, Under review, or Withdrawn

[ccds_strand]
index=7
type=CHAR(1) NULL
comment=which DNA strand contains the observed alleles

[cds_from]
index=8
type=0-based position
comment=coding sequence start position (1-based)

[cds_to]
index=9
type=1-based position
comment=coding sequence ending position (1-based)

[match_type]
index=11
type=VARCHAR(20)
comment=match type, mostly identical.
