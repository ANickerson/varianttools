[linked fields]
hg19=chr, txStart, txEnd

[data sources]
anno_type=range
description=CCDS exons
version=20110420
source_url=ftp://ftp.ncbi.nlm.nih.gov/pub/CCDS/archive/HsGRCH37.2/CCDS_exons.20110420.txt
direct_url=http://vtools.houstonbioinformatics.org/annoDB/ccds_exons-20110420.DB.gz
source_type=txt

[chr]
index=1
type=chromosome

[nc_accession]
index=2
type=VARCHAR(255)
comment=nc accession

[ccds_id]
index=3
type=VARCHAR(255)
comment=ccds id

[cds_strand]
index=4
type=CHAR(1) NULL
comment=which DNA strand contains the observed alleles

[exon_ordinal_position]
index=5
type=0-based position
comment=Transcription start position

[txEnd]
index=6
type=1-based position
comment=Transcription end position

[cdsStart]
index=7
type=0-based position
comment=Coding region start

[cdsEnd]
index=8
type=1-based position
comment=Coding region end

[exonCount]
index=9
type=INTEGER NULL
comment=Number of exons

[score]
index=12
type=INTEGER NULL
comment=Score

[name2]
index=13
type=VARCHAR(255)
comment=Alternative name

